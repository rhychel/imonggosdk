<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.nueca.concessio_test">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name">

        <activity
            android:name=".C_Login"
            android:label="@string/app_name"
            android:theme="@style/LoginTheme"></activity>
        <activity
            android:name=".C_History"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Concessio.NoActionBar"/>
        <activity
            android:name=".C_HistoryDetails"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Concessio.NoActionBar"/>
        <activity
            android:name=".TestMultiInput"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Concessio.NoActionBar"/>

        <activity
            android:name=".InitializeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Concessio">
        </activity>

        <activity
            android:name="net.nueca.concessio_test.C_SampleLogin"
            android:label="@string/app_name"
            android:theme="@style/LoginTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.nueca.concessio_test.C_Module"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Concessio.NoActionBar">
        </activity>
        <activity
            android:name="net.nueca.concessio_test.C_Module2"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Concessio">
        </activity>

        <service android:name="net.nueca.imonggosdk.swable.ImonggoSwable"
            android:enabled="true"/>

        <service android:name="net.nueca.imonggosdk.operations.sync.SyncModules"
            android:enabled="true"/>
    </application>

</manifest>